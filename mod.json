    "version": "v1.0.0",
    "id": "sorkopiko.integrated_tsl",
    "name": "Integrated TSL",
    "developer": "SorkoPiko",
    "description": "A mod that integrates the TSL into Geometry Dash.",
    "resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
    "settings": {
        "enable-rank": {
            "name": "Enable Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text.",
            "type": "bool",
            "default": true
        },
        "white-rank": {
            "name": "White Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text in white.",
            "type": "bool",
            "enable-if": "enable-rank",
            "default": false
        }
    },
    "dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.19.0",
            "importance": "required"

        }
    ],
